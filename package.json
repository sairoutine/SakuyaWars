{
  "name": "SakuyaWars",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
	"start": "electron ./public/electron.js",
    "build": "MODE=production webpack --progress --colors",
    "zip": "MODE=production webpack --progress --colors && zip -r ./releases/game.zip ./public/",
    "watch": "webpack-dev-server",
	"build:win": "cd public && electron-packager ./ SakuyaWars --overwrite --platform=win32  --arch=x64 --electron-version=1.6.0 --icon=icon.ico --out ../releases --ignore '(.m4a)'",
    "build:mac": "cd public && electron-packager ./ SakuyaWars --overwrite --platform=darwin --arch=x64 --electron-version=1.6.0 --icon=icon.icns --out ../releases --ignore '(.m4a)'"

  },
  "author": "sairoutine",
  "license": "Unlicense",
  "devDependencies": {
    "@babel/register": "7.0.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-minify": "0.4.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "electron-packager": "12.1.0",
    "electron-prebuilt": "1.4.13",
    "eslint": "5.11.1",
    "eslint-loader": "2.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "webpack": "4.16.5",
    "webpack-build-notifier": "0.1.30",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.14"
  }
}
